Name: <%= @attraction.name %> <br>
Total Number of Users: <%= @attraction.users.count %> <br>
Ticket Requirement: <%= @attraction.tickets %> <br>
Height Requirement: <%= @attraction.min_height %> <br>
Nausea Rating: <%= @attraction.nausea_rating %> <br>
Happiness Rating: <%= @attraction.happiness_rating %> <br>

<%= form_tag user_path(session[:user_id]), method: :patch do %>
  <%= hidden_field_tag :attraction_id, @attraction.id %>
  <%= submit_tag "Go on this ride" %>
<% end %>

<% if @user.admin == true %>
  <%= link_to "Edit Attraction", edit_attraction_path %>
<% end %>
